<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--
       定义应用程序的数据库名称。
       默认情况下，每个数据库名都应该以.db结尾。
       如果你没有用。db来命名你的数据库，LitePal会自动为你加上后缀。
       For example:
       下面写法会生成training.db
       -->
    <dbname value="training" />
    <!--
       定义数据库的版本。
       每次你想要升级数据库，修改映射标记中定义的版本值+ 1，数据库升级将会被自动处理。
       -->
    <version value="33" />

    <!--
        用映射标签指定的modle在表中定义数据模型.
        LitePal会在模型中定义的字段将映射到对应表的列中。
        For example:
        PersonalInfoBean 属于package com.lhzw.bluetooth.bean.PersonalInfoBean
        -->
    <list>
        <mapping class="com.jiangtai.count.bean.Person" />
        <mapping class="com.jiangtai.count.bean.Project" />
        <mapping class="com.jiangtai.count.bean.TaskBean" />
        <mapping class="com.jiangtai.count.bean.Score" />
        <mapping class="com.jiangtai.count.bean.NormalPerson" />
        <mapping class="com.jiangtai.count.bean.NormalScore" />
        <mapping class="com.jiangtai.count.bean.LocationInfoBean" />
        <mapping class="com.jiangtai.count.bean.SosBean" />
        <mapping class="com.jiangtai.count.bean.BeiDouReceiveBean" />
        <mapping class="com.jiangtai.count.bean.UploadMajorScoreData" />
        <mapping class="com.jiangtai.count.bean.SendConfirmIdMessage" />
        <mapping class="com.jiangtai.count.bean.ReceiverConfirmIdMessage" />
        <mapping class="com.jiangtai.count.bean.BeiDouSendMessageBean"/>
        <mapping class="com.jiangtai.count.bean.LocationReceiver"/>

        <mapping class="com.jiangtai.count.bean.WeatherInfoBean"/>
        <mapping class="com.jiangtai.count.bean.DeviceFeelInfoBean"/>
        <mapping class="com.jiangtai.count.bean.DeviceInfoBean"/>
        <mapping class="com.jiangtai.count.bean.OilInfoBean"/>
        <mapping class="com.jiangtai.count.bean.AirDropInfo"/>
        <mapping class="com.jiangtai.count.bean.CountRecordBean"/>
        <mapping class="com.jiangtai.count.bean.CarFixBean"/>
        <mapping class="com.jiangtai.count.bean.HelicopterOilInfoBean"/>
        <mapping class="com.jiangtai.count.bean.TransformInfo"/>


    </list>


    <cases value="keep"></cases>

    <storage value="internal" />
</litepal>
